{
    "name": "bitwarden-to-1password",
    "version": "0.0.2",
    "description": "Converts BitWarden exported logins to 1Password-compatible csv format",
    "main": "build/main.js",
    "author": "Sam Huynh <samhwang2112.dev@gmail.com>",
    "license": "ISC",
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=7.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samhwang/bitwarden-to-1password.git"
    },
    "scripts": {
        "prepare": "husky install",
        "clean:all": "npm run prebuild && npm run pretest",
        "prebuild": "rimraf build",
        "build": "ts-node scripts/build.ts",
        "pretest": "rimraf coverage",
        "test:only": "jest",
        "typecheck": "tsc",
        "test": "concurrently --kill-others-on-fail npm:typecheck npm:test:only",
        "lint": "eslint . --ext .ts",
        "lint:fix": "npm run eslint -- --fix",
        "prettier": "prettier -w lib/* scripts/* bin/*",
        "format": "npm run lint && npm run prettier"
    },
    "dependencies": {
        "commander": "^9.4.1",
        "zod": "^3.19.1"
    },
    "devDependencies": {
        "@jest/types": "^29.3.1",
        "@types/jest": "^29.2.3",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "concurrently": "^7.5.0",
        "esbuild": "^0.15.13",
        "esbuild-jest": "^0.5.0",
        "eslint": "^8.27.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
